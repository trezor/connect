[include]
.*/node_modules/hd-wallet/lib
.*/node_modules/trezor-link
.*/node_modules/bchaddrjs/

[ignore]
.*/node_modules/protobufjs-old-fixed-webpack/src/bower.json
.*/node_modules/hd-wallet/lib/utils/stream.js.flow
.*/_old/.*
.*/npm/.*
.*/npm-extended/.*
.*/build/.*
.*/tests/*

[libs]
./node_modules/trezor-link/flowtype/chrome.js
./node_modules/trezor-link/flowtype/webusb.js
./src/flowtype/

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
esproposal.export_star_as=enable
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
suppress_comment=\\(.\\|\n\\)*\\$ExpectError
esproposal.decorators=ignore
module.name_mapper='.*\(.less\)' -> 'CSSModule'
module.system=haste
