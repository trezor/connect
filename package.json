{
  "name": "trezor-connect",
  "version": "5.0.8",
  "author": "TREZOR <info@trezor.io>",
  "homepage": "https://github.com/trezor/connect",
  "description": "High-level javascript interface for TREZOR hardware wallet.",
  "license": "LGPL-3.0+",
  "repository": {
    "type": "git",
    "url": "git://github.com/trezor/connect.git"
  },
  "bugs": {
    "url": "https://github.com/trezor/connect/issues"
  },
  "keywords": [
    "TREZOR",
    "trezor-connect",
    "javascript"
  ],
  "main": "dist/trezor-connect.js",
  "bin": {
    "bump": "./node_modules/version-bump-prompt/bin/bump",
    "karma": "./node_modules/karma/bin/karma"
  },
  "scripts": {
    "dev": "babel-node ./webpack/server.js",
    "dev:popup": "babel-node ./webpack/server.js --config popup",
    "build": "rm -rf dist && webpack --config ./webpack/webpack.config.prod.babel.js --progress",
    "build:quick": "rm -rf dist && webpack --config ./webpack/webpack.config.prod.babel.js --env=quick --progress",
    "stats": "webpack --config ./webpack/webpack.config.prod.babel.js --json > dist/stats.json",
    "test": "karma start karma.conf.js",
    "flow": "flow check src/js",
    "eslint": "eslint src/js",
    "eslint-fix": "eslint --fix src/js",
    "bump": "bump --patch --grep ./package.npm.connect.json"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chokidar": "^1.7.0",
    "copy-webpack-plugin": "^4.4.2",
    "css-loader": "^0.28.10",
    "es6-promise": "^4.2.2",
    "eslint": "^4.12.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.10",
    "flow-bin": "^0.48.0",
    "flow-typed": "^2.1.2",
    "html-webpack-plugin": "^2.30.1",
    "jasmine-ajax": "^3.3.1",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-async": "^0.0.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.5",
    "less": "^3.0.1",
    "less-loader": "^4.0.6",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "open": "^0.0.5",
    "pre-build-webpack": "^0.1.0",
    "sharedworker-loader": "^1.1.1",
    "style-loader": "^0.20.2",
    "uglify-es": "3.3.4",
    "uglifyjs-webpack-plugin": "1.1.8",
    "version-bump-prompt": "^4.0.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.5.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.2",
    "webpack-module-hot-accept": "^1.0.5",
    "worker-loader": "^1.1.1",
    "yargs": "^11.0.0"
  },
  "dependencies": {
    "bip44-constants": "^4.0.1",
    "bitcoinjs-lib-zcash": "^3.0.0",
    "bowser": "^1.9.3",
    "hd-wallet": "^7.11.3",
    "parse-uri": "^1.0.0",
    "trezor-link": "^1.3.8",
    "whatwg-fetch": "^2.0.3",
    "window-or-global": "^1.0.1"
  }
}